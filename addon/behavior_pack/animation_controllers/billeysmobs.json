					"transitions": [
						{
							"doing_thing": "!q.is_in_water"
						}
					],
					"on_exit": [
						"/effect @p water_breathing 5 0 true"
					]
				},
				"doing_thing": {
					"transitions": [
						{
							"default": "q.has_rider"
						}
					],
					"on_exit": [
