                "bananafire": "controller.animation.billeys_mobs.bananafire",
                "gold": "controller.animation.billeys_mobs.duck.gold",
                "flyfook": "controller.animation.fook.fly",
                "ornithologist": "animation.banana_duck.ornithologist",
                "can_sleep": "controller.animation.billeys_mobs.can_sleep"
            },
            
