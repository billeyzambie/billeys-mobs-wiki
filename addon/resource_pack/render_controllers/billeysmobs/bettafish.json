{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.billeys_mobs.terraphin": {
            "geometry": "Geometry.default",
            "arrays": {
                "textures": {
                    "array.base": [
                        "texture.zero",
                        "texture.one",
                        "texture.two",
                        "texture.three"
                    ],
                    "array.patterns": [
                        "texture.zeroa",
                        "texture.onea",
                        "texture.twoa",
                        "texture.threea",
                        "texture.zerob",
                        "texture.oneb",
                        "texture.twob",
                        "texture.threeb",
                        "texture.zeroc",
                        "texture.onec",
                        "texture.twoc",
                        "texture.threec",
                        "texture.zerod",
                        "texture.oned",
                        "texture.twod",
                        "texture.threed"
                    ]
                }
            },
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "part_visibility": [
                {
                    "back_fin": "q.variant"                    
                },
                {
                    "tail_fin": "q.variant"                    
                }
                //white ones shouldnt have a tail fluke and a top fin
                //since they spawn in cold biomes and it would make them lose heat
            ],
            "textures": [
                "q.get_name == 'lol' ? array.base[q.life_time] : array.base[q.variant]",
                "q.get_name == 'lol' ? array.patterns[q.life_time*1.33] : array.patterns[v.mark_variant]"
            ],
            "overlay_color": {
          "r": "q.is_illager_captain ? (math.sin(q.life_time * 200 + q.variant * 234 + 0.0) * 0.5 + 0.5)",
          "g": "q.is_illager_captain ? (math.sin(q.life_time * 200 + q.variant * 234 + 120) * 0.5 + 0.5)",
          "b": "q.is_illager_captain ? (math.sin(q.life_time * 200 + q.variant * 234 + 240) * 0.5 + 0.5)",
                "a": "q.is_illager_captain ? 0.4 : 0.0"
            }
        },
